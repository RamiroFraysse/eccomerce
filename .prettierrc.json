{
  "arrowParens": "avoid",
  "semi": true,
  "singleQuote": false,
  "tabWidth": 4,
  "trailingComma": "all",
  "import/order": [
    "builtin",
    "external",
    "internal",
    "parent",
    "sibling",
    "index"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "quoteProps": "consistent",
  "bracketSpacing": false
}